{
  "name": "raphaelbottura",
  "publisher": "raphaelbottura",
  "displayName": "p5-starter",
  "description": "same",
  "version": "1",
  "repository": "https://github.com/rbottura/p5-starter",
  "engines": {
    "vscode": "^1.95.3"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "keywords": [
    "p5.js",
    "p5",
    "generator",
    "project",
    "template"
  ],
  "icon": "images/asterisk.png",
  "activationEvents": [
    "p5-starter.createStarterProject",
    "workspaceContains:sketch.js"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "p5-starter.createStarterProject",
        "title": "Create p5.js starter project"
      }
    ]
  },
  "extensionPack": [
    "ritwickdey.liveserver"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src -c ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/chai": "^5.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.5.0",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.17.0",
    "glob": "^7.2.3",
    "globals": "^15.14.0",
    "js-yaml": "^3.14.1",
    "mocha": "^10.7.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vscode-test": "^1.6.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "got": "^11.8.5",
    "tsc": "^2.0.4",
    "vsce": "^2.15.0"
  },
  "author": "rbottura <bottura.raphael@gmail.com>",
  "license": "MIT",
  "private": false
}
